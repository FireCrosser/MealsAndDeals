= content_for :head do
  = javascript_include_tag 'courses'

%div.tabs-wrapper
  %ul.nav.nav-pills{ role: "tablist", id: "weekday-tabs" }

  %div.tab-content{ id: "weekday-panes" }

%div.modal.fade{ id: "coursesModal", role: "dialog" }
  %div.modal-dialog{ role: "document" }
    %div.modal-content
      %div.modal-header
        %button.close{ type: "button", data: { dismiss: "modal" }, aria: { label: "Close" }}
          %span{aria: { hidden: "true" }} &times;
        %h4.modal-title Courses
